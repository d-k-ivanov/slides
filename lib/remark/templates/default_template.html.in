<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<link rel="shortcut icon" href="favicon.ico"/>
		<title>@REMARK_TITLE@</title>
		<script src="scripts/jquery.js"></script>
		<style>
			@import url("@REMARK_STYLE@");
		</style>
	</head>
    <body>
		<textarea id="source">

layout: true
<!-- <div class="header"></div> -->
<div class="footer"><p><span>&#60;</span>Dmitriy Ivanov<span>&#62;</span>&#9;| @REMARK_TITLE@ </p></div>

---

@REMARK_CONTENT@
		</textarea>
		<script src="scripts/remark.js"></script>
		<script>
			var hljs = remark.highlighter.engine;
		</script>
		@REMARK_LANGUAGES@
		@REMARK_SCRIPT@
		<script>
			var slideshow = remark.create({
				ratio: '16:9',
				countIncrementalSlides: false,
				highlightLanguage: 'cpp',
				highlightStyle: 'gruvbox-dark',
				highlightLines: true,
				highlightSpans: true,
				slideNumberFormat: '%current%'
			});
		</script>
		<script src="scripts/pointer.laser.js"></script>
	</body>
</html>
